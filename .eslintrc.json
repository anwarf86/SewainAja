{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-tabs":"off",
        "no-underscore-dangle":"off",
        "no-mixed-spaces-and-tabs":"off",
        "import/no-unresolved":"off",
        "no-unused-vars":"off"
    }
}
